<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.ImageInput?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.VideoController">
<children>     
    <ImageView fitHeight="500.0" fitWidth="800.0" pickOnBounds="true">
         <image>
            <Image url="@../image/back.gif" />
         </image>
      </ImageView>
 <ImageView fitHeight="80.0" fitWidth="129.0" layoutX="359.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/logo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="86.0" fitWidth="189.0" layoutX="543.0" layoutY="-8.0" pickOnBounds="true">
         <image>
            <Image url="@../image/NamePanel.png" />
         </image>
      </ImageView>
      <Label layoutX="565.0" layoutY="25.0" prefHeight="30.0" prefWidth="71.0" text="Welcome" textFill="WHITE">
         <font>
            <Font name="Showcard Gothic" size="14.0" />
         </font>
      </Label>
      <Label fx:id="label_name" layoutX="638.0" layoutY="25.0" prefHeight="30.0" prefWidth="91.0" text="Gamer's" textFill="WHITE">
         <font>
            <Font name="Showcard Gothic" size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="70.0" fitWidth="140.0" layoutX="179.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/scorePanel.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="56.0" fitWidth="140.0" layoutX="45.0" layoutY="15.0" pickOnBounds="true">
         <image>
            <Image url="@../image/levelPanel.png" />
         </image>
      </ImageView>
      <Label fx:id="scoreLabel" layoutX="253.0" layoutY="29.0" prefHeight="23.0" prefWidth="28.0" text="40" textFill="WHITE">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Label>
      <Label fx:id="levelLabel" layoutX="121.0" layoutY="28.0" prefHeight="23.0" prefWidth="20.0" text="3" textFill="WHITE">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Label>
       <ImageView fitHeight="38.0" fitWidth="41.0" layoutX="527.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/student.png" />
         </image>
      </ImageView>
      <ImageView fx:id="close" fitHeight="40.0" fitWidth="40.0" layoutX="750.0" layoutY="15.0" onMouseClicked="#exitApp" onMouseEntered="#exitAppEnterMouse" onMouseExited="#exitAppExitMouse" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/close_1.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="335.0" fitWidth="495.0" layoutX="102.0" layoutY="104.0" pickOnBounds="true">
         <image>
            <Image url="@../image/graphic-tablet%20(2).png" />
         </image>
      </ImageView>
      <MediaView fx:id="mediaView" fitHeight="277.0" fitWidth="357.0" layoutX="212.0" layoutY="130.0" preserveRatio="false" />
      <Button fx:id="playButton" layoutX="120.0" layoutY="151.0" mnemonicParsing="false" onAction="#playMedia" onMouseEntered="#playEnterMouse" onMouseExited="#playExitMouse" prefHeight="54.0" prefWidth="63.0">
         <effect>
            <ImageInput>
               <source>
                  <Image url="/image/playButton.png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
      <Button fx:id="pauseButton" layoutX="119.0" layoutY="227.0" mnemonicParsing="false" onAction="#pauseMedia" onMouseEntered="#pauseEnterMouse" onMouseExited="#pauseExitMouse" prefHeight="54.0" prefWidth="63.0">
         <effect>
            <ImageInput>
               <source>
                  <Image url="/image/pauseButton.png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
      <Button fx:id="resetButton" layoutX="118.0" layoutY="303.0" mnemonicParsing="false" onAction="#resetMedia" onMouseEntered="#resetEnterMouse" onMouseExited="#resetExitMouse" prefHeight="54.0" prefWidth="63.0">
         <effect>
            <ImageInput>
               <source>
                  <Image url="/image/resetButton.png" />
               </source>
            </ImageInput>
         </effect>
      </Button>
      <ImageView fitHeight="166.0" fitWidth="209.0" layoutX="593.0" layoutY="283.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/boy%20(7).gif" />
         </image>
      </ImageView>
      <!--DELETE-->
      <ImageView fx:id="backButton" fitHeight="78.0" fitWidth="80.0" layoutX="21.0" layoutY="406.0" onMouseClicked="#backButtonClickMouse" onMouseEntered="#backEnterMouse" onMouseExited="#backExiteMouse" pickOnBounds="true" rotate="180.0">
         <image>
            <Image url="@../image/backButton.png" />
         </image>
      </ImageView>
      </children>
</AnchorPane>
